<script type="text/javascript">
    $(document).ready(function () {
        $('#PropertyListingView_BranchId').change(function () {
            $.ajax({
                type: "post",
                url: "/Property/PropertyListing/GetAgentForBranch",
                data: { Id: $('#PropertyListingView_BranchId').val() },
                datatype: "json",
                traditional: true,
                success: function (data) {
                    var district = "<select id='ddlAgent' class='form-control'>";
                    district = district + '<option value="" >--Select--</option>';
                    for (var i = 0; i < data.length; i++) {
                        district = district + '<option value=' + data[i].Id + '>' + data[i].FirstName + '</option>';
                    }
                    district = district + '</select>';
                    $('#dropAgent').html(district);
                }
            });
        });
    });
</script>
