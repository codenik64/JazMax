                                                @Html.ValidationMessageFor(model => model.PropertyImagesView.Imagebytes, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <br />



                                        <br />

                                        <ul class="list-inline pull-right">
                                            <li><button type="button" class="btn btn-default prev-step">Previous</button></li>
                                            <li><button type="button" class="btn btn-primary btn-info-full next-step">Save and continue</button></li>
                                        </ul>
                                    </div>
                                </div>

                                <div class="tab-pane" role="tabpanel" id="complete">
                                    <div class="step33">

                                        <h5><strong>Listing Features & Youtube Media Details</strong></h5>
                                        <hr>
                                        <div class="row">
                                            <div style="margin-right:180px; margin-left:180px;">
                                                <div class="col-md-6">
                                                    @Html.LabelFor(model => model.PropertyListingFeatureView.PropertyFeatureId)
                                                    @Html.ListBoxFor(model => model.PropertyListingFeatureView.PropertyFeatureId, JazMaxDropDownList.GetAllPropertyFeatures(), htmlAttributes: new { @class = "form-control", multiple = "multiple" , id = "Chosen" })
                                                    @Html.ValidationMessageFor(model => model.PropertyListingFeatureView.PropertyFeatureId, "", new { @class = "text-danger" })
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                @Html.LabelFor(model => model.PropertyListingYoutubeView.YoutubeVideoLink)
                                                @Html.EditorFor(model => model.PropertyListingYoutubeView.YoutubeVideoLink, new { htmlAttributes = new { @class = "form-control" } })
                                                @Html.ValidationMessageFor(model => model.PropertyListingYoutubeView.YoutubeVideoLink, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                        <ul class="list-inline pull-right">
                                            <li><button type="button" class="btn btn-default prev-step">Previous</button></li>
                                            
                                            <li><input type="submit" value="Complete Listing Upload" class="btn btn-primary btn-info-full" onclick="save();" /></li>
                                        </ul>
                                    </div>
                                </div>

                           
                            </div>
                        </form>
                    </div>
                </section>
            </div>
        </div>
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

