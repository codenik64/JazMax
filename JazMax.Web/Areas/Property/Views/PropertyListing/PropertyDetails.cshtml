<script>
    $("#show").click(function () {
        $("#Enquire").toggle();
    });
</script>
<script>
    function ResetLeadForm() {
        $('#FullName').val(null);
        $('#ContactNumber').val(null);
        $('#Email').val(null);
        $('#Comments').val(null);
        $('#devBox').hide()
    }

    function CreateLead() {

        var count = 0;

        if (count == 0) {
   
            $.ajax({
                type: 'Post',
                dataType: 'Json',
                data:
                {
                    FullName: $('#FullName').val(),
                    ContactNumber: $('#ContactNumber').val(),
                    Email: $('#Email').val(),
                    Comments: $('#Comments').val(),
                    ProptyId: @Model.PropertyListingView.PropertyListingId
                },
                url: '/Leads/Leads/CreateNewLead',
                success: function (da) {
                    if (da.Result == "Success") {

                        $('#devBox').show()
                        ResetLeadForm();

                    }
                    else {

                        alert('Error' + da.Message);
                    }
                },
                error: function (da) {
                    debugger;
                    alert('Error');
                }
            });
        }


    }
</script>