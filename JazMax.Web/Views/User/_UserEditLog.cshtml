@model IEnumerable<JazMax.Web.ViewModel.ChangeLog.EditLogView>

@{
    ViewBag.Title = "GetLog";
}

@if (Model.Count() > 0)
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TableColumn)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.ValueBefore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValueAfter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChangeDate)
            </th>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TableColumn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ValueBefore)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ValueAfter)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ChangeDate)
                </td>

            </tr>
        }
    </table>
}
else
{
    <p>No edit history</p>
}