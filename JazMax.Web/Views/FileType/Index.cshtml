@model IEnumerable<JazMax.Web.ViewModel.Documents.CoreFileTypesView>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateFileType")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CoreFileTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CoreFileTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CoreFileTypeId })
        </td>
    </tr>
}

</table>
